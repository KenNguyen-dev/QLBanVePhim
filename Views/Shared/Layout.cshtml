                                if (FormId == '#register') {
                                    $('.user-form.register').css({ 'display': 'none' });
                                    $('.user-form.modify').css({ 'display': 'block', 'opacity': 0 });
                                    if (movie.movie_hour != '') {
                                        order();
                                    }
                                } else {
                                    $('.tab-register').css({ 'display': 'block' });
                                    $('.block-user .user-form.register').css({ 'display': 'none' });
                                }
                                $('body').append('<div  class="contact-success color-blue">' + data.message + '</div>');

                                changeText();
                                $('html, body').animate({ scrollTop: 0 }, 'slow');
                                setTimeout(hidemsg, 5000);
