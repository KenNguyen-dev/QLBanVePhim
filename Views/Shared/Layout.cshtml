        function changeText() {
            var tmp;
            $('.register-content .btn-login').addClass('btn-logout');
            tmp = $('.register-content .btn-login a').text();
            $('.register-content .btn-login a').text($('.register-content .btn-login a').attr('data-tmp'));
            $('.register-content .btn-login a').attr('data-tmp', tmp);

            $('.register-content .btn-register').addClass('btn-userinfo');
            tmp = $('.register-content .btn-register a').text();
            $('.register-content .btn-register a').text($('.register-content .btn-register a').attr('data-tmp'));
            $('.register-content .btn-register a').attr('data-tmp', tmp);
        }
        $(document).ready(function () {
            refreshcaptcha();
            //setTimeout(getcode,1000);
            $(document).on('click', '#btn-send-top, #btn-send-tab', function () {
                var FormId = "";
                var Validate = "";
                var Url = BASE_URL + "?route=cms/user/forgot";
                if ($(this).attr('id') == 'btn-send-top') {
                    FormId = "#forgot-top";
                    Validate = validateReset('');
                } else {
                    FormId = "#forgot";
                    Validate = validateReset('1');
                }

                if (Validate == true) {
                    var dataPost = $(FormId).serialize();
                    getPage(Url, 'POST', dataPost, function (data) {
                        $('.loadicon').fadeOut(300, 'linear', function () {
                            $('.loadicon').remove();
                        });
                        data = JSON.parse(data);
                        if (data.status == 200) {
                            $('body').append('<div  class="contact-success color-blue">' + data.message + '</div>');
                            $('html, body').animate({ scrollTop: 0 }, 'slow');
                            setTimeout(hidemsg, 5000);
                        } else {
                            $('body').append('<div  class="contact-success color-blue">' + data.message + '</div>');
                            $('html, body').animate({ scrollTop: 0 }, 'slow');
                            setTimeout(hidemsg, 5000);
                        }

                    });
                }
            });

            $(document).on('click', '#btn-save-top, #btn-save-tab', function () {
                var FormId = "";
                var Validate = "";
                var Url = BASE_URL + "?route=cms/user/update";
                if ($(this).attr('id') == 'btn-save-top') {
                    FormId = "#modify-top";
                    Validate = validateUpdate('');
                } else {
                    FormId = "#modify";
                    Validate = validateUpdate('1');
                }

                if (Validate == true) {
