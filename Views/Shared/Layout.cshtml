                            } else {
                                $('body').append('<div  class="contact-success color-blue">' + data.message + '</div>');
                                $('html, body').animate({ scrollTop: 0 }, 'slow');
                                setTimeout(hidemsg, 5000);
                            }

                        });
                    } else {
                        $('body').append('<div  class="contact-success color-blue">Vui lòng chọn đồng ý các điều khoản</div>');
                        $('html, body').animate({ scrollTop: 0 }, 'slow');
                        setTimeout(hidemsg, 5000);
                    }
                }
            });
            $('#btn-login-tab, #btn-login-top').click(function () {
                var valid = false;
                var FormId = "";
                if ($(this).attr('id') == 'btn-login-top') {
                    valid = validateLogin1();
                    FormId = "#login";
                } else {
                    valid = validateLogin();
                    FormId = "#login-top";
                }
                var $this = $('.register-wrap .btn-login');
                if (valid == true) {
                    $('body').append('<div class="loadicon" style="display:block"><span class="circle"></span></div>');
                    var datapost = $(FormId).serialize();
                    var url = "https://cinestar.com.vn/dangnhap";

                    $.ajax({
                        type: "POST",
                        url: url,
                        data: datapost,

                        success: function (success) {
                            $('.loadicon').fadeOut(300, 'linear', function () {
                                $('.loadicon').remove();
                            });
                            $('.contact-success').remove();
                            var json = JSON.parse(success);
                            if (json.status == 200) {
                                USERNAME = json.user.NAME;
                                EMAIL = json.user.EMAIL;
                                PHONE = json.user.PHONE;

                                changeText();
                                isLogin = true;
                                $(FormId).find("input[type=text], textarea").val("");
                                $(FormId + ' .focus-text.hide').removeClass('hide');
                                $('body').append('<div  class="contact-success color-blue">Đăng nhập thành công.</div>');

                                //dong form login
                                $('.register-wrap ul li').removeClass('active');
                                $('.block-user .tab-register, .block-user .tab-login').fadeOut(200);
                                $('.register-content').removeClass('showbox');

                                if (movie.movie_hour != '') {
                                    order();
                                }

                                //$('.user-people').addClass('active');
                                $('html, body').animate({ scrollTop: 0 }, 'slow');
                            } else {
                                $('body').append('<div  class="contact-success color-red">Đăng nhập thất bại.</div>');
                            }
                            $('html, body').animate({ scrollTop: 0 }, 'slow');
                            setTimeout(hidemsg, 5000);
                        }
                    });  //end ajax
                    return false;
                }//end if

                $(".formError").click(function () {
                    $(this).hide();
                });

                return false;
            });


            //$('#btn-contact-reset').click(function(){hideerror();});
            $("#phone").numeric();
            //$("#fax").numeric();

            $('#frm_contact').keydown(function (e) {
                if (e.keyCode == 13) {
