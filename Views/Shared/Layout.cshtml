                            $('html, body').animate({ scrollTop: 0 }, 'slow');
                            setTimeout(hidemsg, 5000);
                        }

                    });
                }
            });
            $('#btn-register-top, #btn-register-tab').on('click', function () {
                var FormId = "";
                var Validate = "";
                var Url = BASE_URL + "dangky";
                var accept = "";
                if ($(this).attr('id') == 'btn-register-top') {
                    FormId = "#register-top";
                    Validate = validateRegister('');
                    accept = "#member_accept";
                } else {
                    FormId = "#register";
                    Validate = validateRegister('1');
                    accept = "#member_accept1";
                }

                if (Validate == true) {
                    if ($(accept).is(":checked")) {
                        var dataPost = $(FormId).serialize();
